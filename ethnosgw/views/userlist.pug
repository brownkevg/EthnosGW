extends layout

block content
	img.bg(src='/images/concert.jpg')
	include nav
	.container.index
		img#logo-white(src='/images/logo-white.png')
		p Harness the power of globalized technology to empower every nation, tribe, and tongue to experience and explore new worship expressions.
		#search-upload
			#search-box
				img(alt="search" src="/images/search-white.png")
				a(href="/search") SEARCH CATALOGUE
			.bar
			#upload-box
				img(alt="upload" src="/images/upload-white.png")
				a(href="#") UPLOAD A RESOURCE
		h1.
			User List
		ul
			each user, i in userlist
				li
					a(href="mailto:#{user.email}")= user.username

